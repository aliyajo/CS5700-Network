#!/usr/bin/env -S python3 -u

def compare_ips(ip1, ip2):
    ip1 = ip1.split('/')[0]
    ip2 = ip2.split('/')[0]
    # Split the IP addresses into their components
    ip1_parts = ip1.split('.')
    ip2_parts = ip2.split('.')
    
    # Convert each part of the IP address to integers
    ip1_int = [int(part) for part in ip1_parts]
    ip2_int = [int(part) for part in ip2_parts]
    
    # Compare the numerical values of the IP addresses
    for i in range(4):
        if ip1_int[i] > ip2_int[i]:
            return f"{ip1} is larger than {ip2}"
        elif ip1_int[i] < ip2_int[i]:
            return f"{ip2} is larger than {ip1}"
    
    return "Both IP addresses are equal"

# Example usage
ip = "192.0.0.0/24"
ip2 = "192.0.0.0/24"
cidr = ip.split('/')[0] + "/" + str((int(ip.split('/')[1]) - 1))
print(cidr)

print(compare_ips(ip, ip2))



  
